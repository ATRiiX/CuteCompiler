; ModuleID = 'main'
source_filename = "main"

@string = private unnamed_addr constant [9 x i8] c"i is %d \00"
@string.1 = private unnamed_addr constant [1 x i8] zeroinitializer
@string.2 = private unnamed_addr constant [9 x i8] c"j is %d \00"
@string.3 = private unnamed_addr constant [1 x i8] zeroinitializer
@string.4 = private unnamed_addr constant [9 x i8] c"k is %d \00"
@string.5 = private unnamed_addr constant [1 x i8] zeroinitializer

declare i32 @printf(i8*)

declare i32 @puts(i8*)

define i32 @main() {
entry:
  %0 = alloca i32
  store i32 0, i32* %0
  %1 = alloca i32
  store i32 0, i32* %1
  %2 = alloca i32
  br i1 true, label %then, label %ifcont

then:                                             ; preds = %entry
  store i32 1, i32* %0
  br label %ifcont

ifcont:                                           ; preds = %then, %entry
  %arrayPtr = load i32, i32* %1
  %3 = load i32, i32* %1
  %cmptmp = icmp ult i32 %3, 2
  %4 = icmp ne i1 %cmptmp, false
  br i1 %4, label %forloop, label %forcont

forloop:                                          ; preds = %forloop, %ifcont
  %arrayPtr1 = load i32, i32* %1
  %5 = load i32, i32* %1
  %addtmp = add i32 %5, 1
  store i32 %addtmp, i32* %1
  %arrayPtr2 = load i32, i32* %1
  %6 = load i32, i32* %1
  %cmptmp3 = icmp ult i32 %6, 2
  %7 = icmp ne i1 %cmptmp3, false
  br i1 %7, label %forloop, label %forcont

forcont:                                          ; preds = %forloop, %ifcont
  store i32 0, i32* %2
  %arrayPtr5 = load i32, i32* %2
  %8 = load i32, i32* %2
  %cmptmp6 = icmp ult i32 %8, 3
  %9 = icmp ne i1 %cmptmp6, false
  br i1 %9, label %forloop4, label %forcont11

forloop4:                                         ; preds = %forloop4, %forcont
  %arrayPtr7 = load i32, i32* %2
  %10 = load i32, i32* %2
  %addtmp8 = add i32 %10, 1
  store i32 %addtmp8, i32* %2
  %arrayPtr9 = load i32, i32* %2
  %11 = load i32, i32* %2
  %cmptmp10 = icmp ult i32 %11, 3
  %12 = icmp ne i1 %cmptmp10, false
  br i1 %12, label %forloop4, label %forcont11

forcont11:                                        ; preds = %forloop4, %forcont
  %arrayPtr12 = load i32, i32* %0
  %13 = load i32, i32* %0
  %calltmp = call i32 @printf([9 x i8]* @string, i32 %13)
  %calltmp13 = call i32 @puts([1 x i8]* @string.1)
  %arrayPtr14 = load i32, i32* %1
  %14 = load i32, i32* %1
  %calltmp15 = call i32 @printf([9 x i8]* @string.2, i32 %14)
  %calltmp16 = call i32 @puts([1 x i8]* @string.3)
  %arrayPtr17 = load i32, i32* %2
  %15 = load i32, i32* %2
  %calltmp18 = call i32 @printf([9 x i8]* @string.4, i32 %15)
  %calltmp19 = call i32 @puts([1 x i8]* @string.5)
  ret i32 0
}

